{
  "$id": "projectAgreement",
  "type": "object",
  "properties": {
    "code": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "subject": {
      "type": "string"
    },
    "agreementType": {
      "type": "string"
    },
    "startDate": {
      "oneOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    },
    "endDate": {
      "oneOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    },
    "budget": {
      "type": "number"
    },
    "contribution": {
      "type": "number"
    },
    "partners": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "pis": {
      "type": "array",
      "items": {
