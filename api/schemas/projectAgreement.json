        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "oneOf": [
              {
                "enum": [
                  ""
                ]
              },
              {
                "format": "email"
              }
            ]
          },
          "name": {
            "type": "string"
          },
          "surname": {
            "type": "string"
          }
